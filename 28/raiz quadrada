#include <stdio.h>
#include <math.h>


int main()
{
    float num, verificar_raiz;
    
    printf("Digite um numero: ");
    scanf("%f", &num);
    
    if (num >= 0 ){
        verificar_raiz = sqrt(num);
         printf("A raiz quadrada de %f eh: %.2lf\n", num, verificar_raiz);
    }
    
    return 0;
}
