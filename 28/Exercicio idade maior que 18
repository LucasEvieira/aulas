int main()
{
    int idade;
    int contador = 0;
    int i = 0;
    
    printf("Digite a idade de 5 pessoas");
    
    while (i <5){
        printf("Pessoa %d: ", i+1);
        scanf("%d", &idade);
        
        if (idade >= 18) {
            contador++;
        }

        i++;
    }

    printf("O número de pessoas com mais de 18 anos é: %d\n", contador);

    }
    

